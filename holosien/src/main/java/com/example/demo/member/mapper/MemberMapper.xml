<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
 
<mapper namespace="com.example.demo.board.mapper.MemberMapper">
 
<insert id="insertMembers" useGeneratedKeys="true"
    keyProperty="idmember">
  insert into member (email,password,name,gender,age,manner)
  values (#{email},#{password},#{name},#{gender},#{age},#{manner})
</insert>

</mapper>